   "source": [
    "# Write a Python program to reverse a string"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "4b5a5d05",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The reverse string is dcba4321\n"
     ]
    }
   ],
   "source": [
    "def rev(str):  #Declaring the function\n",
    "    str1 = \"\"   # Declaring the string\n",
    "    for i in str:  \n",
    "        str1 = i + str1  \n",
    "    return str1      \n",
    "     \n",
    "str = \"1234abcd\"    # Given String       \n",
    "print(\"The reverse string is\",rev(str)) "
   ]
  }
